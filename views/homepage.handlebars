
<form class="card-body" id="search-form">

    <input class="form-input" id="keyword" name="comment-body" required></input>
    <button type="submit" class="btn btn-primary" id="submitButton">Submit</button>
</form>

    {{#each result as |data|}}

            <a href="/venue/{{data.id}}">{{data.id}}</a>
            <h2>{{{data.name}}}</h2>
            <img src={{data.mainImage.url}}></img>

        {{/each}}

<script src="/js/search.js"></script>
